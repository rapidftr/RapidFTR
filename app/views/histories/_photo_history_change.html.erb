<li>
  <div>
	<%= datetime %>
	<% unless new_photos.blank? %>
          <%= new_photos.size > 1 ? "Photos" : "Photo" %>
          <% new_photos.each do |new_photo| %>
            <%= link_to_photo_with_key(new_photo) %> 
          <% end %>
          added by <%= user_name %> belonging to <%= organisation %>
	<%end%>
	<% unless deleted_photos.blank? %>
          <%= deleted_photos.size > 1 ? "Photos" : "Photo" %>
          <% deleted_photos.each do |deleted_photo| %>
            <%= link_to_photo_with_key(deleted_photo) %> 
          <% end %>
          deleted by <%= user_name %> belonging to <%= organisation %>
	<%end%>
  </div>
</li>

