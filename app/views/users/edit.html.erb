<h1 class="no_border">Users</h1>
<div class="page-content-new">
  <div class="side-tab">
    <ul>
      <li class="current"><%= link_to "Users", users_path %></li>
      <li><%= link_to "Roles", roles_path %></li>
    </ul>
  </div>
  <div class="side-tab-content">
    <h2><%= link_to 'List of Users', users_path %> &gt; <%= @user.user_name %></h2>
    <%= render :partial => "editable_user", :object => @user %>
    <% if is_admin? %>
        <%= render :partial => "mobile_login_history", :object => @user %>
    <% end %>
  </div>
</div>
